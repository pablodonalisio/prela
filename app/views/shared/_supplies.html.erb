<h4><%= title %></h4>
<div class="row">
  <div class="w-auto">
    <h5 class="text-center">Bateria</h5>
    <% if equipment.battery.present? %>
      <%= render "shared/supply", supply: equipment.battery %>
    <% else %>
      <%= render "shared/supply_placeholder", path: edit_equipment_path(equipment.id) %>
    <% end %>
  </div>
</div>