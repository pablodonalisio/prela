<%= turbo_frame_tag "main_content", class: "w-100" do %>
  <div class="row justify-content-around mt-4 gy-5">
    <div class="col-md-10 text-bg-secondary rounded p-3">
      <%= render "location_equipments/control_panel", overdue_ups: @overdue_ups, overdue_power_units: @overdue_power_units %>
    </div>
    <% if current_user.admin? %>
      <div class="col-md-10 text-bg-secondary rounded p-3">
        <%= render "links/links", links: @links %>
      </div>
    <% end %>
  </div>
<% end %>

